{
  "name": "react-hierarchy-explorer",
  "displayName": "React Hierarchy Explorer",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.65.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:reactHierarchy",
    "onCommand:react-hierarchy-explorer.helloWorld"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "reactHierarchy.refreshEntry",
        "title": "Refresh",
        "icon": "$(refresh)"
      },
      {
        "command": "reactHierarchy.focusRelatedFile",
        "title": "Focus Related File",
        "icon": "$(export)"
      },
      {
        "command": "reactHierarchy.printDiagnostic",
        "title": "Run Diagnostic"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "reactHierarchy",
          "name": "React Hierarchy Explorer"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "reactHierarchy",
        "contents": "No stuff!"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "reactHierarchy.refreshEntry",
          "when": "view == reactHierarchy",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "reactHierarchy.focusRelatedFile",
          "when": "view == reactHierarchy && viewItem == node",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "yarn run compile-web",
    "vscode:prepublish": "yarn run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/vscode": "^1.65.0",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vscode/test-web": "^0.0.22",
    "assert": "^2.0.0",
    "eslint": "^8.9.0",
    "mocha": "^9.2.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@react-sidekick/ast": "file:.yalc/@react-sidekick/ast"
  }
}
